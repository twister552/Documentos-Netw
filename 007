#include <iostream>
#include <cmath>

using namespace std;

int round_off(float y) {
    
    int whole = y;
    
    float rem = y - whole;
    if (whole >= 0) {
        if (fabs(rem * 10) >= 5) {
            whole++;
        }
    } else {
        if (fabs(rem * 10) >= 5) {
            whole--;
        }
    }
    
    return whole;
}

int main() {
    int n;
    cin>>n;
    
    int ctr = 0;
    while (ctr < n) {
        int y;
        cin>>y;
        
        float ans = (y - 32) * (5 / 9.0);
        cout<<round_off(ans)<<" ";
        ctr++;
    }
    
    return 0;
}
 
